{
  "name": "just-us-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cross-env": "^10.1.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.4.7",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-scripts start",
    "build": "npx tailwindcss init -p --override && cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "npx tailwindcss init -p --override && npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
